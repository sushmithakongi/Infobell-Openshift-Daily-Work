    "def binary_search(arr,x):\n",
    "    low=0\n",
    "    high=len(arr)-1\n",
    "    mid=0\n",
    "    \n",
    "    while low<=high:\n",
    "        mid=(low+high)//2\n",
    "        if arr[mid]<x:\n",
    "            low=mid+1\n",
    "        elif arr[mid]>x:\n",
    "            high=mid-1\n",
    "        else:\n",
    "            return mid\n",
    "    return -1\n",
    "\n",
    "arr=[2,3,4,5,6,4]\n",
    "x=4\n",
    "res=binary_search(arr,x)\n",
    "if res!=-1:\n",
    "    print(\"{} is present at {} position\".format(x,res))\n",
    "else:\n",
    "    print(\"Element not found\")"
   ]
